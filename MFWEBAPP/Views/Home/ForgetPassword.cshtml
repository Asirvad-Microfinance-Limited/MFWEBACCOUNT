    <script src="~/lib/bootstrap/dist/assets/js/popper.min.js"></script>
    <script src="~/lib/bootstrap/dist/assets/js/plugins.js"></script>
    <script src="~/lib/bootstrap/dist/assets/js/main.js"></script>
    <script src="~/ClientApp/App.js"></script>
    <script src="~/ClientApp/Services/HttpService.js"></script>
    @*<script src="~/lib/CryptoJSv3.1.2/rollups/aes.js"></script>*@

    <script src="~/ClientApp/ForgetPassword/ForgetPassword.Controller.js"></script>
    <script src="~/ClientApp/ForgetPassword/ForgetPassword.Validator.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/crypto-js/3.1.2/rollups/aes.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/crypto-js/3.1.2/rollups/pbkdf2.js"></script>
</body>
