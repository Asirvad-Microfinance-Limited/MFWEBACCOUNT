    <link href="~/lib/bootstrap/dist/assets/css/themify-icons.css" rel="stylesheet" />
    <link href="~/lib/Plugins/sweetalert/css/sweetalert.css" rel="stylesheet" />
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,600,700,800' rel='stylesheet' type='text/css'>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.2/animate.css" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.2/animate.min.css" rel="stylesheet" />
    <link rel="icon" type="image/x-icon" href="favicon.ico">
    <link href="~/lib/Plugins/LoaderWrapper/Loginstyle.css" rel="stylesheet" />
    <style>

        .form-control {
            /*border-radius: 1.375rem !important;*/
        }

        .btn {
            border-radius: 100px !important;
        }

        html {
            /*background-image: url('https://mac.mactech.net.in/mfweb/lib/bootstrap/dist/images/banking.jpg');*/
            /*background-image: url('lib/bootstrap/dist/images/bg4.jpg');*/
            background-image: url('https://mac.mactech.net.in/mfweb/lib/bootstrap/dist/images/bg4.jpg');
            background-repeat: no-repeat;
            background-size: cover;
            position: relative;
            width: 100%;
            height: 610px;
            margin-left: auto;
            margin-right: auto;
        }
    </style>


</head>
<body class="bg-dark">


    @*<div class="page-loader-wrapper">
        <div class="loader">
            <div class="preloader">
                <div class="spinner-layer pl-blue-grey">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>
            </div>
            <p style="color:#dbb714;font-family:'Myanmar Text'">Please Wait...</p>
        </div>
    </div>*@
    <!-- #END# Page Loader -->
    <div class="sufee-login d-flex align-content-center flex-wrap ">
        <div class="container">
            <div class="login-content">


                <br /> <br /> <br /> <br /> <br /> <br />
                <div class="login-form animated  bounceInLeft">
                    <div class="col-sm-9 text-center" style="margin:0 auto;">

                        @*<div class="login-logo">
                                <a href="https://asirvadmicrofinance.co.in/">
                                    <img class="align-content" src="~/lib/bootstrap/dist/images/loginlogo.png" alt="">
                                </a>
                                <a href="https://www.manappuram.com/">
                                    <img class="align-content" style="height: 11px;width: 178px;" src="data:image/png;base64," alt="">
                                </a>
                            </div>*@

                        <form method="post" id="forgetPassword" name="forgetPassword">
                            <h3 style="color:#e03030e0">Forget your password? </h3>

                            <br />
                            <div class="form-group">

                                <input type="text" id="txtUserID" name="txtUserID" class="form-control form-control-lg" placeholder="User ID" autocomplete="off">
                                <label id="txtUserID-error" class="error error-msg" for="txtUserID" style="display:block"></label>
                            </div>
                            <div class="form-group">
                                <input type="text" placeholder="Mobile" id="mobileNo" name="mobileNo"  maxlength="10" class="form-control form-control-lg">
                                <label id="mobileNo-error" class="error error-msg" for="mobileNo" style="display:block"></label>

                            </div>
                            @*<div id="otpdiv" style="display:none;">*@


                                <div class="form-group" id="otpdiv" style="display:none;">
                                    <input type="text" placeholder="OTP" id="otp" name="otp" class="form-control form-control-lg" maxlength="6">
                                    <label id="otp-error" class="error error-msg" for="otp" style="display:block"></label>


                                </div>

                                <div class="form-group" id="newPswd" style="display:none;">
                                    @*<label for="cc-payment" class="control-label mb-1">New Password<b style="color:red">*</b></label>*@
                                    <input type="password" id="txtnewPass" name="txtnewPass" placeholder="New Password" minlength="8" maxlength="16" class="form-control" />
                                    <label id="txtnewPass-error" class="error" for="txtnewPass" style="display:block"></label>
                                </div>

                            @*</div>*@
                            <br />
                            @*<button type="submit" name="login_submit" id="login_submit" class="btn btn-success btn-lg btn-flat m-b-30 m-t-30" > Sign in </button>*@
                            <input type="submit" id="sendotp" name="sendotp" value="Send OTP" class="btnsmall btn-login btn-lg btn-group-sm m-b-15 m-t-15">
                            <input type="button"  id="resetbtn"  name="resetbtn" value="Save" class="btnsmall btn-login btn-lg btn-group-sm m-b-15 m-t-15" disabled>



                        </form>
                    </div>

                </div>
                <footer class="text-center animated bounceInLeft">
                    <medium style="color:#000000;z-index: 152;">
                        &copy; Copyright @DateTime.Now.Year.ToString(),All rights reserved by Asirvad.Designed by MACOM
                    </medium>
                </footer>
            </div>
        </div>
    </div>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    @*<script src="~/ClientApp/Services/LocalStorageServices.js"></script>*@
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
