                <div class="login-form animated  bounceInLeft">
                    <div class="col-sm-9 text-center" style="margin:0 auto;">

                        <div class="login-logo">
                            <a href="https://asirvadmicrofinance.co.in/" target="_blank">
                                <img class="align-content" src="~/lib/bootstrap/dist/images/loginlogo.png" alt="">
                            </a>
                            <a href="https://www.manappuram.com/" target="_blank">
                                <img class="align-content" style="height: 11px;width: 178px;" src="data:image/png;base64," alt="">
                            </a>
                        </div>

                        <form method="post" id="login" name="login">
                            <div class="form-group">
                                <label></label>

                                <input type="text" id="userid" name="userid" class="form-control form-control-lg" placeholder="User ID" autocomplete="off">
                                <label id="userid-error" class="error error-msg" for="userid" style="display:block"></label>
                            </div>
                            <div class="form-group">
                                <label></label>
                                <input type="password" id="password" name="password" class="form-control form-control-lg" placeholder="Password" minlength="8" maxlength="16" autocomplete="off">
                                <label id="password-error" class="error error-msg" for="password" style="display:block"></label>
                            </div>
                            <div class="checkbox">
                                <label>
                                    @*<input type="checkbox"> Remember Me*@
                                </label>
                                <label class="pull-right">
                                    @*<a href="#">Forgotten Password?</a>*@
                                </label>
                            </div>
                            @*<button type="submit" name="login_submit" id="login_submit" class="btn btn-success btn-lg btn-flat m-b-30 m-t-30" > Sign in </button>*@
                            <input type="submit" value="Login" id="login_submit" name="login_submit" class="btn btn-login btn-lg btn-flat m-b-30 m-t-30">
                            <!--<div class="social-login-content">
                                <div class="social-button">
                                    <button type="button" class="btn social facebook btn-flat btn-addon mb-3"><i class="ti-facebook"></i>Sign in with facebook</button>
                                    <button type="button" class="btn social twitter btn-flat btn-addon mt-2"><i class="ti-twitter"></i>Sign in with twitter</button>
                                </div>
                            </div>-->


                            <div class="register-link m-t-15 text-center">
                                @*<p>Don't have account ? <a href="#"> Sign Up Here</a></p>*@


                            </div>


                        </form>
                    </div>

                </div>

                <div class="modal fade" id="Otp" tabindex="-1" role="dialog" aria-labelledby="more" aria-hidden="true" data-backdrop="static" data-keyboard="false" style="background-color:white;">

                    <div class="modal-dialog modal-md" role="document">

                        <div class="otp-form animated  bounceIn" style="margin-top: 100px;  padding-right: 0px; padding-left: 0px;height:500px; width:500px;background-color:#aaaaa1">
                            <div class="containers" style="padding-left: 61px; height: 20px; display: block "></div>

                            <button type="button" class="close" data-dismiss="modal" id="close" aria-label="Close" style=" padding-right:23px;">
                                <span aria-hidden="true">&times;</span>
                            </button>
                            <div class="otp-group" style="padding-top:5px;">
                                <label></label>
                                <div style="width: 420px; text-align: center; padding-left: 106px; padding-top: 180px;">

                                    <label id="phonenum" class="" for="dtFromDate" style="display:block;font-size:20px;"></label>
                                    <input type="text" id="otpid" style="width:200px" name="otpid" class="rounded-input" placeholder="Enter OTP" maxlength="6" oninput="this.value = this.value.replace(/[^0-9]/g, '').replace(/(\..*)\./g, '$1');">
                                </div>
                            </div>
                            <div style="text-align: center;margin-top: 15px;">
                                <input type="button" value="Verify OTP" id="VerifyOtp" name="VerifyOtp" class="btn btn-login btn-lg btn-flat m-b-20 m-t-20" style="width: auto;">
                                <input type="button" value="Resend OTP" id="ResentOtp" name="ResentOtp" class="btn btn-login btn-lg btn-flat m-b-20 m-t-20" style="width: auto;">
                            </div>
                            <div style="margin-top:-2%;">
                                <label style="text-align: justify; font-size: 78%; color: #000000; padding-left: 21px; padding-top: 34px; padding-right: 21px;">
                                    <span style="color:red;">Note: </span>update your phone number in <a href="https://apps.asirvad.com/hrms_app/login.aspx"><FONT COLOR="#006699">HRM</FONT></a> module if the mentioned phone number didnot function.
                                </label>
                            </div>
                        </div>
                        @*</div>*@
                    </div>
                </div>


                <footer class="text-center animated bounceInLeft" style="padding-top: 10px;">
                    <medium style="color:#000000;z-index: 152;">
                        &copy; Copyright @DateTime.Now.Year.ToString(),All rights reserved by Asirvad.Designed by <a href="http://www.macomsolutions.com/" style="color:#fff;">MACOM</a>
